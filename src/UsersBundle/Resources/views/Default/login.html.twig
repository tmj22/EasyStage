
{% extends 'base.html.twig' %}
{% block titulo %}New User{% endblock %}
{% block contenido %}

    <!-- Begin cta -->
    <section id="cta-section" style="margin-top:60px;">
    	<div class="cta">
    		<div class="container">
    				<div class="row">

    						<div class="col-md-9">
    								<h1>Send us your CV</h1>
    									<p>Inspired by nature, follow technology, appreciate the classics. You can build modern & professional websites with Unika. The possibilities are just endless.</p>
    							</div>

    							<div class="col-md-3">
    								<div>
    							<a class="btn btn-default btn-lg" href="http://www.imransdesign.com/" target="_blank" role="button">Apply now</a>
    								</div>
    							</div>

    					</div> <!-- /.row -->
    			</div> <!-- /.container -->
    	</div>
    </section>
    <!-- End cta -->
    <!-- ========= Begin offers =========-->
    					 <div class="page-header-wrapper" style="margin-top:50px;">
    							 <div class="container">
    									 <div class="page-header text-center">
    											 <h2></h2><br>
    											 <img src="{{ asset('img/puntos.png')}}" class="puntos" align="center">

    									 </div>
    							 </div>
    					 </div>

               <h1>LOGIN</h1>
               <table class="table">
                 <tr>
                   {% if error %}
                   <div class="table">
                     <table>
                     </table
                   </div>
                       <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                   {% endif %}

                   <form action="{{ path('login') }}" method="post">
                       <label for="username">Username:</label>
                       <input type="text" id="username" name="_username" value="{{ last_username }}" />

                       <label for="password">Password:</label>
                       <input type="password" id="password" name="_password" />

                       {#
                           If you want to control the URL the user
                           is redirected to on success (more details below)
                           <input type="hidden" name="_target_path" value="/account" />
                       #}


                       <button type="submit">login</button>
                   </form>
               </table>


							 <!-- ========= End offers =========-->

{% endblock %}
